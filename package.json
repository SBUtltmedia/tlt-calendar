{
  "name": "tlt-calendar",
  "version": "1.0.0",
  "description": "A calendar for TLT employee hours",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i -p 5000",
    "start": "webpack-dev-server --progress --colors --history-api-fallback --no-info --content-base ./ --config webpack.dev.config.babel.js",
    "build": "webpack --config webpack.prod.config.babel.js --output-path /tmp/sccal/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SBUtltmedia/tlt-calendar.git"
  },
  "author": "Ken Fehling",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SBUtltmedia/tlt-calendar/issues"
  },
  "homepage": "https://github.com/SBUtltmedia/tlt-calendar#readme",
  "dependencies": {
    "griddle-react": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "js-md5": "^0.4.1",
    "json2csv": "^3.7.1",
    "keymaster": "^1.6.2",
    "lodash": "^4.13.1",
    "neat-csv": "^2.1.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.0",
    "react-file-download": "^0.3.2",
    "react-redux": "^4.4.5",
    "react-reorder": "^2.0.0",
    "react-router": "^2.5.2",
    "react-router-bootstrap": "^0.23.1",
    "react-widgets": "^3.3.4",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.18.0",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^17.0.0",
    "node-sass": "^3.7.0",
    "react-addons-perf": "^15.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-babel-jest": "^1.0.4",
    "webpack-dev-server": "^2.3.0"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/webpack-babel-jest",
    "testPathDirs": [
      "tests"
    ],
    "testRegex": "/*.test.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  }
}
